<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="BackEnd">
  <class name="BackEnd.Articulo, BackEnd" table="articulos" lazy="true">
    <id name="IdArticulo">
      <column name="idarticulo"/>
      <generator class="native" />
    </id>
    <property name="IdCliente">
      <column name="clientes_id"/>
    </property>
    <property name="Nombre">
      <column name="nombre"/>
    </property>
    <property name="Descripcion">
      <column name="descripcion"/>
    </property>
    <property name="Alto">
      <column name="alto"/>
    </property>
    <property name="Largo">
      <column name="largo"/>
    </property>
    <property name="Ancho">
      <column name="ancho"/>
    </property>
    <property name="Peso">
      <column name="peso"/>
    </property>
    <property name="Actividad">
      <column name="actividad"/>
    </property>
    <property name="Activo">
      <column name="activo"/>
    </property>
  </class>
</hibernate-mapping>