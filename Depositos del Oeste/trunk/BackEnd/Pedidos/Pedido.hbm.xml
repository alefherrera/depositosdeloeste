<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="BackEnd">
  <class name="BackEnd.Pedido, BackEnd" table="Pedidos" lazy="true">
    <id name="Id">
      <column name="id"/>
      <generator class="native"/>
    </id>
    <property name="FechaPedido">
      <column name="fecha_pedido"/>
    </property>
    <property name="IdCliente">
      <column name="clientes_id"/>
    </property>
  </class>
</hibernate-mapping>